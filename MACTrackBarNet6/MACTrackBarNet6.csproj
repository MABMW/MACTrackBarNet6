<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Platforms>AnyCPU;x64;x86</Platforms>
    <PackageIcon>Iconx128.png</PackageIcon>
    <RepositoryUrl>https://github.com/MABMW/MACTrackBarNet6</RepositoryUrl>
    <Description>Un control de barra deslizante, con bordes redondeados
y mas personalizado, para WinForms.Net6</Description>
    <Version>06.01.22</Version>
    <PackageId>$(AssemblyName)$(PlatformTarget)</PackageId>
    <Authors>BeltranMolina</Authors>
    <FileVersion>6.1.22.1</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\..\..\UsersLocal\Imagenes\Iconx128.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
